					<% if  !@userConectatProfileStats[current_user.profile.region]["stats"]["quickplay"]["overall_stats"]["comprank"].nil? %>
						<li class="list-group-item">MMR: <%= @userConectatProfileStats[current_user.profile.region]["stats"]["quickplay"]["overall_stats"]["comprank"] %></li>
						<li class="list-group-item">Divizie: <%= @userConectatProfileStats[current_user.profile.region]["stats"]["quickplay"]["overall_stats"]["tier"].capitalize %></li>
						<li class="list-group-item">Scor Competitiv:<span style="color: green"> <%= @userConectatProfileStats[current_user.profile.region]["stats"]["competitive"]["overall_stats"]["wins"] %></span> - <span style="color: red"><%= @userConectatProfileStats[current_user.profile.region]["stats"]["competitive"]["overall_stats"]["losses"] %> </span>(<%= @userConectatProfileStats[current_user.profile.region]["stats"]["competitive"]["overall_stats"]["win_rate"] %>%)</li>
