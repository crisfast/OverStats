						   <li class="list-group-item">Heal: <%= (@userCautatEroi[@regiuneCautat]["heroes"]["stats"]["quickplay"]["mercy"]["general_stats"]["healing_done"]/(@userCautatEroi[@regiuneCautat]["heroes"]["playtime"]["quickplay"]["mercy"]* 60)).round(2)%> / min</li>						   <li class="list-group-item">K/D: <%= (@userCautatEroi[@regiuneCautat]["heroes"]["stats"]["quickplay"]["mercy"]["general_stats"]["eliminations"].to_i/@userCautatEroi[@regiuneCautat]["heroes"]["stats"]["quickplay"]["mercy"]["general_stats"]["deaths"].to_f).round(2)  %></li>
						   <li class="list-group-item">Acuratete: <%= ((@userCautatEroi[@regiuneCautat]["heroes"]["stats"]["quickplay"]["mercy"]["general_stats"]["weapon_accuracy"]) * 100).to_i %>% </li>
						   <li class="list-group-item">Aliati reinviati: <%= @userCautatEroi[@regiuneCautat]["heroes"]["stats"]["quickplay"]["mercy"]["hero_stats"]["players_resurrected"].to_i %> </li>
					   </ul>
					</div>
				</div>
			</div>
		</div>
<% end %>

