<%= render 'layouts/header'%>
	<%= yield %>

<% if @BlizzProfile["error"] == 404 %>

		<div class="container">
			<div class="row ">
				<div class="profile-sidebar">
					<h2 class="profile-userpic">Profil</h2>
					<!-- SIDEBAR USER TITLE -->
					
					<div class="profile-usertitle" style="margin-top: 5px">
						<div class="profile-usertitle-name">
							<%= @user.name%> <br>
								<div class="profile-usertitle-battletag">(
									<span style="color: red"><%= @profil.battletag.gsub('-','#')%> </span>)</div>
						</div>
						<div class="profile-usertitle-region">
							<%= @profil.region.upcase%> -
								<%= @profil.platform.upcase%> <br>
						</div>
					</div>
					<div class="alert alert-danger" style="text-align: center; font-weight: bold;"> BattleTag-ul introdus de utilizator nu exista</div>
					<!-- END SIDEBAR USER TITLE -->
					<% if user_signed_in? %>
						<% if current_user.profile.id == @id.to_i %>
							<div class="profile-userbuttons">
								<a href="/profiles/<%=@id%>/edit"><button type="button" class="btn btn-success btn-sm">Editeaza</button></a>
							</div>
						<% else %>
							<div class="profile-userbuttons">
								Contul nu este valid, nu puteti sa va comparati!
							</div>
						<%end%>
				   <% end %>
				</div>
			</div>
		</div>

<% else %>
		<div class="container">
			<div class="row ">
				<div class="profile-sidebar">
					<h2 class="profile-userpic">Profil</h2> <br>
					<!-- SIDEBAR USERPIC -->
					<div class="profile-userpic">
						<img src='<%= @BlizzProfile[@profil.region]["stats"]["quickplay"]["overall_stats"]["avatar"]  %>' class="img-responsive" alt="">
					</div>
					<!-- END SIDEBAR USERPIC -->
					<!-- SIDEBAR USER TITLE -->
					<div class="profile-usertitle">
						<div class="profile-usertitle-name">
							<%= @user.name%> <br>
								<div class="profile-usertitle-battletag">(
									<%= @profil.battletag.gsub('-','#') %>)</div>
						</div>
						<div class="profile-usertitle-region">
							<%= @profil.region.upcase%> -
								<%= @profil.platform.upcase%> <br>
						</div>
						<div class="profile-usertitle-level ">
							Level:
							<%=  (@BlizzProfile[@profil.region]["stats"]["quickplay"]["overall_stats"]["prestige"].to_i * 100) + @BlizzProfile[@profil.region]["stats"]["quickplay"]["overall_stats"]["level"].to_i  %>
						</div>
					</div>
					<!-- END SIDEBAR USER TITLE -->
					<% if user_signed_in? %>
						<% if current_user.profile.id == @id.to_i %>
							<div class="profile-userbuttons">
								<a href="/profiles/<%=@id%>/edit"><button type="button" class="btn btn-success btn-sm">Editeaza</button></a>
							</div>
							<% else %>
								<div class="profile-userbuttons">
								<%= link_to 'Comapara-ne', { :controller => "search", :action => "index", :battleTagSearch => @profil.battletag.gsub('-','#'), :platforma => @profil.platform, :regiune => @profil.region} %></button></a>
									
								</div>
								<%end%>
									<% end %>
				</div>
			</div>
		</div>

		<div class="container">
			<div class="row">
				<div class="statistics-user">
					<h2>Statistici Generale</h2> <br><br>

					<ul class="list-group general-section">
					<% if  !@BlizzProfile[@profil.region]["stats"]["quickplay"]["overall_stats"]["comprank"].nil? %>
						<li class="list-group-item">MMR: <%= @BlizzProfile[@profil.region]["stats"]["quickplay"]["overall_stats"]["comprank"] %></li>
						<li class="list-group-item">Divizie: <%= @BlizzProfile[@profil.region]["stats"]["quickplay"]["overall_stats"]["tier"].capitalize %></li>
						<li class="list-group-item">Scor Competitiv:<span style="color: green"> <%= @BlizzProfile[@profil.region]["stats"]["competitive"]["overall_stats"]["wins"] %></span> - <span style="color: red"><%= @BlizzProfile[@profil.region]["stats"]["competitive"]["overall_stats"]["losses"] %> </span>(<%= @BlizzProfile[@profil.region]["stats"]["competitive"]["overall_stats"]["win_rate"] %>%)</li>
					<% else %>
						<li class="list-group-item">Divizie: Nu si-a primit divizia!</li>
						<% if @BlizzProfile[@profil.region]["stats"]["competitive"]["competitive"] == true %>
						<li class="list-group-item">Scor Competitiv:<span style="color: green"> <%= @BlizzProfile[@profil.region]["stats"]["competitive"]["overall_stats"]["wins"] %></span> - <span style="color: red"><%= @BlizzProfile[@profil.region]["stats"]["competitive"]["overall_stats"]["losses"] %> </span>(<%= @BlizzProfile[@profil.region]["stats"]["competitive"]["overall_stats"]["win_rate"] %>%)</li>
						<% end %>
				    <% end %>
						<li class="list-group-item">Victorii in QuickPlay: <span style="color: green"><%= @BlizzProfile[@profil.region]["stats"]["quickplay"]["overall_stats"]["wins"] %> </span></li>
						<li class="list-group-item">Timp petrecut in joc: <%= @BlizzProfile[@profil.region]["stats"]["quickplay"]["game_stats"]["time_played"].to_i %> de ore </li>
						<li class="list-group-item">Eliminari: <%= (@BlizzProfile[@profil.region]["stats"]["quickplay"]["game_stats"]["eliminations"]/(@BlizzProfile[@profil.region]["stats"]["quickplay"]["game_stats"]["time_played"].to_i * 60)).round(2) %> / min</li>
						<li class="list-group-item">Dauna efectuata: <%= (@BlizzProfile[@profil.region]["stats"]["quickplay"]["game_stats"]["damage_done"]/(@BlizzProfile[@profil.region]["stats"]["quickplay"]["game_stats"]["time_played"].to_i * 60)).round(2) %> / min</li>
						<li class="list-group-item">Decese: <%= (@BlizzProfile[@profil.region]["stats"]["quickplay"]["game_stats"]["deaths"]/(@BlizzProfile[@profil.region]["stats"]["quickplay"]["game_stats"]["time_played"].to_i * 60)).round(2) %> / min</li>
						<li class="list-group-item">Heal: <%= (@BlizzProfile[@profil.region]["stats"]["quickplay"]["game_stats"]["healing_done"]/(@BlizzProfile[@profil.region]["stats"]["quickplay"]["game_stats"]["time_played"].to_i * 60)).round(2) %>  hp / min</li>
						<li class="list-group-item">Medalii: <span style="color: saddlebrown"><%= @BlizzProfile[@profil.region]["stats"]["quickplay"]["game_stats"]["medals_bronze"].to_i %></span> - <span style="color: gray"><%= @BlizzProfile[@profil.region]["stats"]["quickplay"]["game_stats"]["medals_silver"].to_i %></span> - <span style="color: goldenrod"><%= @BlizzProfile[@profil.region]["stats"]["quickplay"]["game_stats"]["medals_gold"].to_i %></span></li>
					</ul>
					
				</div>
			</div>
		</div>
	
