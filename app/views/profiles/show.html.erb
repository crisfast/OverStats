						   <li class="list-group-item">Inamici adormiti: <%= @HeroesProfile[@profil.region]["heroes"]["stats"]["quickplay"]["ana"]["general_stats"]["enemies_slept"] %></li>
					   </ul>
				  </div>
			   </div>
		   </div>
		</div>
      </section>
<% end %>

